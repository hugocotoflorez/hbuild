default:
    cc      gcc
    flags   -Wall -Wextra -fsanitize=address,null
    src     ./src/*
    inc     ./include
    filename hbuild
    exec     compile install

compile:
    exec    1
    1       $cc -I $inc $flags $src -o $filename

install:
    exec    1
    1       mv ./$filename ~/.local/bin/$filename

git:
    exec    add commit push
    add     git add .
    commit  git commit -m "Auto commit"
    push    git push
